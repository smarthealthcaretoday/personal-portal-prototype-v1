            <xs:element name="CodeSystemVersion" type="xs:string" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>The version of the code system (if not included in the OID)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:group>
    <xs:group name="TimeStampsGroup">
        <xs:annotation>
            <xs:documentation>The group container for different forms of time stamps</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="StartStamp" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>The start stamp for timestamp intervals (e.g. the start of a sick leave). Both the date and time can be specified.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="StartDate" type="xs:date" minOccurs="0" maxOccurs="1" nillable="true" />
                        <xs:element name="StartTime" type="xs:time" minOccurs="0" maxOccurs="1" nillable="true" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="EndStamp" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>The end stamp for time stamp intervals (e.g. the end of an antibiotic medication). Both the end date and end time can be specified.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="EndDate" type="xs:date" minOccurs="0" maxOccurs="1" nillable="true" />
                        <xs:element name="EndTime" type="xs:time" minOccurs="0" maxOccurs="1" nillable="true" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="PointStamp" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>A single time stamp for non-interval time stamps (e.g. the sampling date/time for most laboratory tests).</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="PointDate" type="xs:date" minOccurs="0" maxOccurs="1" nillable="true" />
                        <xs:element name="PointTime" type="xs:time" minOccurs="0" maxOccurs="1" nillable="true" />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:group>
</xs:schema>
